{
  "name": "3d-ai-generator-app",
  "version": "1.0.0",
  "main": "main.js",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.545.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.30.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^31.7.7",
    "postcss": "^8.5.6",
    "react-scripts": "^5.0.1",
    "tailwindcss": "^3.4.18",
    "wait-on": "^7.2.0"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "concurrently \"npm run react-dev\" \"npm run electron-dev\"",
    "react-dev": "cross-env BROWSER=none react-scripts start",
    "electron-dev": "cross-env ELECTRON_START_URL=http://localhost:3000 wait-on http://localhost:3000 && electron .",
    "start:prod": "npm run build && electron .",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}