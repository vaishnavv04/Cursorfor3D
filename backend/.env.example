# Environment Configuration for CursorFor3D Backend

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/cursorfor3d

# JWT Secret for Authentication
JWT_SECRET=your_jwt_secret_key_here

# Blender TCP Connection
BLENDER_TCP_HOST=127.0.0.1
BLENDER_TCP_PORT=9876

# Multiple Gemini API Keys (Random Selection)
# You can use up to 9 keys. The system will randomly select one for each API call.
# If a key fails, it will be placed in cooldown for 5 minutes and retried with another key.

GEMINI_API_KEY_1=your_first_gemini_api_key_here
GEMINI_API_KEY_2=your_second_gemini_api_key_here
GEMINI_API_KEY_3=your_third_gemini_api_key_here
GEMINI_API_KEY_4=your_fourth_gemini_api_key_here
GEMINI_API_KEY_5=your_fifth_gemini_api_key_here
GEMINI_API_KEY_6=your_sixth_gemini_api_key_here
GEMINI_API_KEY_7=your_seventh_gemini_api_key_here
GEMINI_API_KEY_8=your_eighth_gemini_api_key_here
GEMINI_API_KEY_9=your_ninth_gemini_api_key_here

# Optional: Single Gemini API Key (Fallback if no numbered keys found)
# GEMINI_API_KEY=your_single_gemini_api_key_here

# Other AI Provider API Keys
GROQ_API_KEY=your_groq_api_key_here
COHERE_API_KEY=your_cohere_api_key_here

# Integration API Keys
SKETCHFAB_API_KEY=your_sketchfab_api_key_here
HYPER3D_API_KEY=your_hyper3d_api_key_here

# Rate Limiting Configuration
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX=30

# Code Cache Configuration
CODE_CACHE_MAX=100
CODE_CACHE_TTL_MS=300000

# Server Configuration
PORT=5000
